{
  "name": "@mbolli/datastar-attribute-prop",
  "version": "1.0.6",
  "description": "Datastar plugin for custom attribute binding to sync element properties with signals",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "files": [
    "dist/index.js",
    "dist/index.d.ts",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && NODE_ENV=production node esbuild.config.js && dts-bundle-generator --config dts-bundle-generator.config.cjs",
    "build:dev": "tsc && node esbuild.config.js && dts-bundle-generator --config dts-bundle-generator.config.cjs",
    "test": "node --test dist/index.test.js",
    "test:compile": "tsc && node --test dist/index.test.js",
    "test:browser": "python3 -m http.server 8080",
    "prepublishOnly": "pnpm build && pnpm test"
  },
  "keywords": [
    "datastar",
    "plugin",
    "attribute",
    "property",
    "binding",
    "reactive"
  ],
  "author": "Michael Bolli <michael@bolli.us>",
  "license": "MIT",
  "devDependencies": {
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.10.0",
    "datastar": "github:starfederation/datastar",
    "dts-bundle-generator": "^9.5.1",
    "esbuild": "^0.25.12",
    "jsdom": "^27.1.0",
    "typescript": "^5.9.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mbolli/datastar-attribute-prop.git"
  }
}
